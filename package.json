{
  "name": "fab-o2o",
  "version": "0.0.1",
  "private": true,
  "description": "Super App - Food Delivery, Ride-hailing, Dine-in QR",
  "scripts": {
    "dev": "pnpm --filter @fab-o2o/* dev",
